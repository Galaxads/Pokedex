<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity2"
    android:background="#DC0A2D"
    tools:ignore="ExtraText">

    <EditText
        android:id="@+id/editTextText"
        android:layout_width="268dp"
        android:layout_height="48dp"
        android:background="@drawable/search"
        android:drawableLeft="@drawable/search1"
        android:ems="10"
        android:hint="Search"
        android:inputType="text"
        android:padding="10dp"

        android:textSize="13dp"


        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.188"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.099"
        tools:ignore="TextSizeCheck" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="369dp"
        android:layout_height="52dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.642"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023"
        app:srcCompat="@drawable/title" />

    <Button
        android:id="@+id/button"
        android:layout_width="66dp"
        android:layout_height="53dp"
        android:backgroundTint="@color/white"
        android:text="А"
        android:textColor="#DC0A2D"
        android:textSize="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.953"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.092" />

    <ScrollView
        android:layout_width="392dp"
        android:layout_height="582dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="26dp"
        android:layout_marginEnd="11dp"
        android:layout_marginBottom="7dp"
        android:background="@drawable/search"
        android:contentDescription="@string/appbar_scrolling_view_behavior"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextText"
        app:layout_constraintVertical_bias="0.0">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">




            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <ImageButton
                    android:id="@+id/imageButton12"
                    android:layout_width="98dp"
                    android:layout_height="152dp"
                    android:layout_weight="1"
                    android:background="@color/white"
                    android:contentDescription="@string/appbar_scrolling_view_behavior"
                    app:srcCompat="@drawable/list_item__9_" />

                <ImageButton
                    android:id="@+id/imageButton3"
                    android:layout_width="132dp"
                    android:layout_height="152dp"
                    android:background="@color/white"
                    android:contentDescription="@string/appbar_scrolling_view_behavior"
                    app:srcCompat="@drawable/list_item" />

                <ImageButton
                    android:id="@+id/imageButton6"
                    android:layout_width="98dp"
                    android:layout_height="152dp"

                    android:layout_weight="1"
                    android:background="@color/white"
                    android:contentDescription="@string/appbar_scrolling_view_behavior"
                    app:srcCompat="@drawable/list_item__4_" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <ImageButton
                    android:id="@+id/imageButton4"
                    android:layout_width="130dp"
                    android:layout_height="152dp"
                    android:background="@color/white"
                    android:contentDescription="@string/appbar_scrolling_view_behavior"
                    app:srcCompat="@drawable/list_item__2_" />

                <ImageButton
                    android:id="@+id/imageButton9"
                    android:layout_width="98dp"
                    android:layout_height="152dp"
                    android:layout_weight="1"
                    android:background="@color/white"
                    android:contentDescription="@string/appbar_scrolling_view_behavior"
                    app:srcCompat="@drawable/list_item__7_" />

                <ImageButton
                    android:id="@+id/imageButton8"
                    android:layout_width="98dp"
                    android:layout_height="152dp"
                    android:layout_weight="1"
                    android:background="@color/white"
                    android:contentDescription="@string/appbar_scrolling_view_behavior"
                    app:srcCompat="@drawable/list_item__6_" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <ImageButton
                    android:id="@+id/imageButton11"
                    android:layout_width="98dp"
                    android:layout_height="152dp"
                    android:layout_weight="1"
                    android:background="@color/white"
                    android:contentDescription="@string/appbar_scrolling_view_behavior"
                    app:srcCompat="@drawable/list_item__8_" />

                <ImageButton
                    android:id="@+id/imageButton7"
                    android:layout_width="98dp"
                    android:layout_height="152dp"
                    android:layout_weight="1"
                    android:background="@color/white"

                    android:contentDescription="@string/appbar_scrolling_view_behavior"
                    app:srcCompat="@drawable/list_item__5_" />

                <ImageButton
                    android:id="@+id/imageButton5"
                    android:layout_width="98dp"
                    android:layout_height="152dp"
                    android:layout_weight="1"
                    android:background="@color/white"
                    android:contentDescription="@string/appbar_scrolling_view_behavior"
                    app:srcCompat="@drawable/list_item__3_" />

            </LinearLayout>

        </LinearLayout>
    </ScrollView>>

</androidx.constraintlayout.widget.ConstraintLayout>